{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"semi": false,
	"experimentalTernaries": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"quoteProps": "consistent",
	"trailingComma": "all",
	"singleAttributePerLine": true,
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": false,
	"proseWrap": "always",
	"insertPragma": false,
	"requirePragma": false,
	"tabWidth": 2,
	"useTabs": true,
	"embeddedLanguageFormatting": "auto",
	"printWidth": 100
}
